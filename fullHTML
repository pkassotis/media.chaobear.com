            <!-- TWITTER FEED -->
            <div class="side_menu_item">
                <div class="side_menu_item_title">{text:Sidebar Title Twitter Feed}</div>

                <div class="side_menu_item_twitter">
                    <div id="tweets"></div>
                    <script type="text/javascript">
                        function recent_tweets(data) {
                            String.prototype.parseURL = function() {
                                return this.replace(/[A-Za-z]+:\/\/[A-Za-z0-9-_]+\.[A-Za-z0-9-_:%&\?\/.=]+/g, function(url) {
                                    return url.link(url);
                                });
                            };
                                    
                            String.prototype.parseUsername = function() {
                                return this.replace(/[@]+[A-Za-z0-9-_]+/g, function(u) {
                                    var username = u.replace("@","")
                                    return u.link("https://twitter.com/"+username);
                                });
                            };
                                    
                            String.prototype.parseHashtag = function() {
                                return this.replace(/[#]+[A-Za-z0-9-_]+/g, function(t) {
                                    var tag = t.replace("#","%23")
                                    return t.link("https://twitter.com/hashtag/"+tag+"?src=hash");
                                });
                            };
                                    
                            for (i=0; i<{text:Sidebar Twitter Feed Limit}; i++) {
                                document.getElementById("tweets").innerHTML =
                                document.getElementById("tweets").innerHTML + '<div class="twitter-content">' + data[i].text.parseURL().parseUsername().parseHashtag() + '</div>'
                            }
                        }
                    </script>
                                
                    <script type="text/javascript" src="/tweets.js"></script>
                </div>
            </div>
            <!-- TWITTER FEED -->
            {/block:IfEnableSidebarTwitterFeed}

            {block:IfEnableSidebarInstagram}
