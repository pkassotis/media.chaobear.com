					<div class="theme-permalink-post-box-full"{block:Photo} style="width:100%"{/block:Photo}{block:Photoset} style="width:100%"{/block:Photoset}{block:Panorama} style="width:100%"{/block:Panorama}>
						{block:IfDisqusShortname}
						<!-- POST - DISQUS -->
						<div class="theme-post-disqus">
							<div id="disqus_thread"></div>
							<script type="text/javascript">
								var disqus_shortname = '{text:Disqus Shortname}';
										
								(function() {
									var dsq = document.createElement('script'); dsq.type = 'text/javascript'; dsq.async = true;
										dsq.src = '//' + disqus_shortname + '.disqus.com/embed.js';
									(document.getElementsByTagName('head')[0] || document.getElementsByTagName('body')[0]).appendChild(dsq);
								})();
							</script>
						</div>
						<!-- POST - DISQUS -->
						{/block:IfDisqusShortname}

						{block:PostNotes}
						<!-- POST - NOTES -->
						<div class="theme-post-notes">
							{PostNotes}
						</div>
						<!-- POST - NOTES -->
						{/block:PostNotes}
					</div>
				{/block:Date}
				
                {/block:Posts}
            </div>
        </div>
    </div>
