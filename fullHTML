    <!-- INDEX JS -->
    <script type="text/javascript">
        (function($){
        "use strict";

        /* === SETTING === */
        var themeApp = {
            theme_full_section_slider:function(){
                $('.full-section-slider-container').slick({
                    arrows: true,
                    fade: false,
                    speed: 300,
                    autoplay: {select:Autoplay Full Product Slider},
                    autoplaySpeed: 3000,
                    infinite: false,
                    responsive: [
                        {
                          breakpoint: 800,
                          settings: {
                            slidesToShow: 1,
                            slidesToScroll: 1,
                            infinite: true,
                            dots: true,
                            arrows: false
                          }
                        }
                    ]
                });
            },
            theme_activity_left:function(){
                $('.theme-activity-left').slick({
                    arrows: false,
                    fade: true,
                    speed: 300,
                    autoplay: {select:Autoplay Activity Left Slider},
                    autoplaySpeed: 3000
                });
            },
            theme_activity_nav_left:function(){
                $('#activity-nav-left-prev').on('click', function(e){
                    e.preventDefault();
                    $('.theme-activity-left').slickPrev();
                    return false;
                });  

                $('#activity-nav-left-next').on('click', function(e){
                    e.preventDefault();
                    $('.theme-activity-left').slickNext();
                    return false;
                });  
            },
            theme_activity_right:function(){
                $('.theme-activity-right').slick({
                    arrows: false,
                    fade: false,
                    speed: 300,
                    autoplay: {select:Autoplay Activity Right Slider},
                    autoplaySpeed: 3000
                });
            },
            theme_activity_nav_right:function(){
                $('#activity-nav-right-prev').on('click', function(e){
                    e.preventDefault();
                    $('.theme-activity-right').slickPrev();
                    return false;
                });  

                $('#activity-nav-right-next').on('click', function(e){
                    e.preventDefault();
                    $('.theme-activity-right').slickNext();
                    return false;
                });  
            },
            theme_partners:function(){
                $('.theme-partners-slider').slick({
                    arrows: false,
                    fade: false,
                    speed: 300,
                    autoplay: {select:Autoplay Featured Work Slider},
                    autoplaySpeed: 3000,
                    infinite: false,
                    slidesToShow: 3,
                    slidesToScroll: 3,
                    draggable: true,
                    dots: true,
                    responsive: [
                        {
                          breakpoint: 960,
                          settings: {
                            slidesToShow: 2,
                            slidesToScroll: 2,
                            infinite: true,
                            dots: true
                          }
                        },
                        {
                          breakpoint: 675,
                          settings: {
                            slidesToShow: 1,
                            slidesToScroll: 1,
                            infinite: true,
                            dots: true
                          }
                        }
                    ]
                });
            },
            theme_process:function(){
                $('#process-tabs').organicTabs();
            },
            theme_skills:function(){
                $('#skills-accordion-list').accordion();
            },
            theme_fun_facts:function(){
                {block:IfEnableSectionFunFact}
                $('#theme-skills').waypoint(function(){ 
                    animate_number();
                },{ 
                    offset: '80%',
                    triggerOnce: true
                });
                {/block:IfEnableSectionFunFact}

                {block:IfEnableSectionSkills}
                /* PROGRESS BAR FUNCTION */
                function progressBar(percent, $element){
                    var progressBarWidth = percent * $element.width() / 100;
                    $element.find('.progress-width').animate({ width: progressBarWidth }, 500).html("<span>"+ percent + "%</span>");
                }

                function animate_number(){
                    $("#theme-skills .loading").removeClass("loading");

                    /* PROGRESS/SKILL BAR */
                    {block:IfEnableSkillBarRight1}
                    progressBar({text:Skill Bar Right Total 1}, $('#progressBar1'));
                    {/block:IfEnableSkillBarRight1}

                    {block:IfEnableSkillBarRight2}
                    progressBar({text:Skill Bar Right Total 2}, $('#progressBar2'));
                    {/block:IfEnableSkillBarRight2}

                    {block:IfEnableSkillBarRight3}
                    progressBar({text:Skill Bar Right Total 3}, $('#progressBar3'));
                    {/block:IfEnableSkillBarRight3}

                    {block:IfEnableSkillBarRight4}
                    progressBar({text:Skill Bar Right Total 4}, $('#progressBar4'));
                    {/block:IfEnableSkillBarRight4}

                    {block:IfEnableSkillBarRight5}
                    progressBar({text:Skill Bar Right Total 5}, $('#progressBar5'));
                    {/block:IfEnableSkillBarRight5}

                    $('.timer').countTo();
                }
                {/block:IfEnableSectionSkills}
            },
            theme_team:function(){
                $('.theme-team-slider').slick({
                    arrows: false,
                    fade: false,
                    speed: 300,
                    autoplay: {select:Autoplay Team Slider},
                    autoplaySpeed: 3000,
                    infinite: false,
                    slidesToShow: 4,
                    slidesToScroll: 4,
                    draggable: true,
                    dots: true,
                    responsive: [
                        {
                          breakpoint: 960,
                          settings: {
                            slidesToShow: 3,
                            slidesToScroll: 3,
                            infinite: true,
                            dots: true
                          }
                        },
                        {
                          breakpoint: 768,
                          settings: {
                            slidesToShow: 2,
                            slidesToScroll: 2,
                            infinite: true,
                            dots: true
                          }
                        },
                        {
                          breakpoint: 675,
                          settings: {
                            slidesToShow: 1,
                            slidesToScroll: 1,
                            infinite: true,
                            dots: true
                          }
                        }
                    ]
                });
            },
            theme_testimonials:function(){
                $('.theme-testimonials-slider').slick({
                    arrows: false,
                    fade: false,
                    speed: 300,
                    autoplay: {select:Autoplay Testimonial Slider},
                    autoplaySpeed: 3000,
                    infinite: true,
                    draggable: true,
                    dots: true
                });
            },
            theme_clients:function(){
                $('.theme-clients-slider').slick({
                    arrows: false,
                    fade: false,
                    speed: 300,
                    autoplay: {select:Autoplay Client Logo Slider},
                    autoplaySpeed: 3000,
                    infinite: false,
                    draggable: true,
                    slidesToShow: 5,
                    slidesToScroll: 5,
                    dots: true,
                    responsive: [
                        {
                          breakpoint: 960,
                          settings: {
                            slidesToShow: 3,
                            slidesToScroll: 3,
                            infinite: true,
                            dots: true
                          }
                        },
                        {
                          breakpoint: 675,
                          settings: {
                            slidesToShow: 2,
                            slidesToScroll: 2,
                            infinite: true,
                            dots: true
                          }
                        },
                        {
                          breakpoint: 540,
                          settings: {
                            slidesToShow: 1,
                            slidesToScroll: 1,
                            infinite: true,
                            dots: true
                          }
                        }
                    ]
                });
            },
            theme_flickr:function(){
                $('#footer-content-flickr').jflickrfeed({
                    limit: {text:Footer Flickr Limit},
                    qstrings: {
                        id: '{text:Footer Flickr Username}'
                    },
                    itemTemplate: '<li><a href="{{image_b}}"><img src="{{image_s}}" alt="{{title}}" /></a></li>'
                },
                function(data){
                    $('#footer-content-flickr a').colorbox({
                        photo: true,
                        scalePhotos: true,
                        maxHeight:'100%',
                        maxWidth:'100%'
                    });
                });
            },
            theme_posts:function(){
                var $container = $("#theme-posts-loop");

                $container.isotope({
                    itemSelector:'.theme-posts-item',
                    layoutMode: 'fitRows'
                });

                $container.infinitescroll({
                    navSelector: "#keleus_load_more:last",
                    nextSelector: "a#keleus_load_more:last",
                    itemSelector: "#theme-posts-loop .theme-posts-item",
                    behavior:'twitter',
                    loading: {
                        finishedMsg: 'No more posts to load',
                        msgText: '',
                        selector: '#loading_pagination',
                        img: 'https://static.tumblr.com/omz03v3/U6ancs0uc/loader.gif'
                    }
                }, 
                function (newElements) {
                    $container.isotope('appended', $(newElements));
                    themeApp.theme_posts_first_image_generate();
            
                    $(newElements).each(function(){
                        var postplus = $(this).attr('class').replace(/\+/g, "-");
                        $(this).removeClass($(this).attr('class'));
                        $(this).addClass(postplus);
                    });
                });
                
                // Replace Data-Filter Plus
                $('#post-filters > ul > li > a').each(function(){
                    var filterLink = $(this).attr('data-filter');
                    var filterLink2 = filterLink.replace(/\ /g, "-");
                    if($(this).hasClass('active')){
                        $(this).replaceWith('<a href="#" data-filter="'+filterLink2+'" class="active">'+$(this).text()+'</a>');
                    }
                    else{
                        $(this).replaceWith('<a href="#" data-filter="'+filterLink2+'">'+$(this).text()+'</a>');
                    }
                });

                // Replace Class Plus With Minus
                $('.theme-posts-item').each(function(){
                    var postplus = $(this).attr('class').replace(/\+/g, "-");
                    $(this).removeClass($(this).attr('class'));
                    $(this).addClass(postplus);
                });

                // bind filter button click
                $('#post-filters a').click(function(e){
                    e.preventDefault();

                    var filterValue = $(this).attr('data-filter');

                    $('#post-filters a').removeClass('active');
                    $(this).addClass('active');

                    $container.isotope({
                        filter: filterValue
                    });

                    return false;
                });   
            },
            theme_posts_first_image_create:function(src, alt, url, title, description){
                var firstimg = String('');
                firstimg += '<div class="post-text-first-image-container">';
                    firstimg += '<div class="post-text-first-image-hover">';
                        firstimg += '<div class="post-text-first-caption">';
                            firstimg += '<div class="post-text-first-caption-middle">';
                                if(title != null){
                                    firstimg += '<div class="post-text-first-caption-title"><a href="'+url+'">'+title+'</a></div>';
                                }
                                else{
                                    firstimg += '';
                                }

                                if(description != null){
                                    firstimg += '<div class="post-text-first-caption-body">'+description+'</div>';
                                }
                                else{
                                    firstimg += '';
                                }

                                firstimg += '<div class="post-text-first-caption-button"><a href="'+url+'">VIEW MORE</a></div>';
                            firstimg += '</div>';
                        firstimg += '</div>';
                    firstimg += '</div>';
                    firstimg += '<div class="post-text-first-image" style="background-image:url('+src+');"></div>';
                firstimg += '</div>';
                return firstimg;
            },
            theme_posts_first_image_generate:function(){
                function generateImg(){
                    $('.theme-posts-item').each(function(){
                        var post_container = $('.post-text-body');
                        
                        var image = $(this, post_container).find('img:first');
                        var imagesrc = image.attr('src');
                        var imagealt = image.attr('alt');
                        
                        var title = $(this).find('.post-text-body-title h3 a').attr('title');
                        var hyperlink = $(this).find('.permalink_button .right a').attr('href');
                        var description = $(this).find('.post-text-body-content').text();

                        if(typeof imagesrc !== 'undefined'){
                            $(this).find('.post-text').append(themeApp.theme_posts_first_image_create(imagesrc,imagealt,hyperlink,title,description));
                            $(this).find('.post-text-body').remove();
                            $(this).find('.post-text .permalink_button').remove();
                        }
                    });
                }

                generateImg();
            },
            theme_right_sidebar:function(){
                $('.side_menu_button').jPushMenu();

                $(".side_menu_container").niceScroll({
                    cursorborder:"{color:Sidebar Scroll}",
                    cursorcolor:"{color:Sidebar Scroll}",
                    railalign: "left",
                    autohidemode: true
                });
            },
            theme_instagram:function(){
                $("#side_menu_instagram").jqinstapics({
                    "user_id": "{text:Sidebar Instagram UserID}",
                    "access_token": "{text:Sidebar Instagram Access Token}",
                    "count": {text:Sidebar Instagram Limit}
                });
            },
            theme_grid:function(){
                $('.photoset-grid').photosetGrid({
                    highresLinks: true,
                    rel: $('.photoset-grid').attr('data-id'),
                    gutter: '10px',
                    onInit: function(){
                        $('.photoset-loading').css({'display':'block'});
                    },
                    onComplete: function(){
                        $('.photoset-loading').remove();
                        $('.photoset-grid').attr('style', '');
                        $('.photoset-grid a').colorbox({
                            photo: true,
                            scalePhotos: true,
                            maxHeight:'100%',
                            maxWidth:'100%'
                        });
                    }
                });
            },
            theme_responsive_navigation:function(){
                $('#kelnav_subcat').click(function(e){
                    e.preventDefault();
                    $('#kelnav_concat').slideToggle('medium');
                    $('#kelnav_conpages').slideUp();
                });

                $('#kelnav_subpages').click(function(e){
                    e.preventDefault();
                    $('#kelnav_conpages').slideToggle('medium');
                    $('#kelnav_concat').slideUp();
                });
            },
            theme_fitvids:function(){
                $("#fitvids-player").fitVids();
            },
            theme_init:function(){
                {block:IfEnableSectionFullProduct}
                    themeApp.theme_full_section_slider();
                {/block:IfEnableSectionFullProduct}

                {block:IfEnableSectionActivity}
                    themeApp.theme_activity_left();
                    themeApp.theme_activity_nav_left();
                    themeApp.theme_activity_right();
                    themeApp.theme_activity_nav_right();
                {/block:IfEnableSectionActivity}

                {block:IfEnableSectionFeaturedWorks}
                    themeApp.theme_partners();
                {/block:IfEnableSectionFeaturedWorks}

                {block:IfEnableSectionWorkflowProcess}
                    themeApp.theme_process();
                {/block:IfEnableSectionWorkflowProcess}

                themeApp.theme_fun_facts();

                {block:IfEnableSectionSkills}
                    themeApp.theme_skills();
                {/block:IfEnableSectionSkills}

                {block:IfEnableSectionTeam}
                    themeApp.theme_team();
                {/block:IfEnableSectionTeam}

                {block:IfEnableSectionTestimonial}
                    themeApp.theme_testimonials();
                {/block:IfEnableSectionTestimonial}

                {block:IfEnableSectionClientLogo}
                    themeApp.theme_clients();
                {/block:IfEnableSectionClientLogo}

                {block:IfEnableFooterFlickr}
                    themeApp.theme_flickr();
                {/block:IfEnableFooterFlickr}

                {block:IndexPage}
                    themeApp.theme_posts();
                    themeApp.theme_posts_first_image_generate();
                {/block:IndexPage}

                {block:IfEnableSidebar}
                    themeApp.theme_right_sidebar();
                {/block:IfEnableSidebar}

                {block:IfEnableSidebarInstagram}
                    themeApp.theme_instagram();
                {/block:IfEnableSidebarInstagram}

                themeApp.theme_responsive_navigation();
                themeApp.theme_grid();
                themeApp.theme_fitvids();
            }
        }

        /* === INITIALING === */
        $(document).ready(function(){
            themeApp.theme_init();

            {block:IndexPage}
            if($('#theme-navigation').hasClass('trans')){
                $('body').addClass('responsive-transparent');
            }
            else if($('#theme-navigation').hasClass('no-transparent')){
                $('body').addClass('body-has-no-transparent');
            }
            else if($('#theme-navigation').hasClass('no-transparent no-fixed')){
                $('body').addClass('body-has-no-transparent');
            }

            $('#theme-slider .flexslider').flexslider({
                animation: "fade",
                directionNav: false,
                controlNav: true,
                slideshow: {select:Autoplay Banner Slider},
                useCSS: false,
                keyboard: true,
                slideshowSpeed: 5000
            });
            {/block:IndexPage} 
        });

        }(jQuery));

        /* === AUTOMATE MENU === */
        function init() {
            window.addEventListener('scroll', function(e){
                var distanceY = window.pageYOffset || document.documentElement.scrollTop,
                shrinkOn = 65,
                header = document.querySelector("#theme-navigation");
                if (distanceY > shrinkOn) {
                    classie.add(header,"smaller");
                } else {
                    if (classie.has(header,"smaller")) {
                        classie.remove(header,"smaller");
                    }
                }
            });
        }

        window.onload = init();


        /* FLEXSLIDER */
        $(window).load(function(){
            $.stellar({
                horizontalScrolling: false,
                verticalOffset: 40,
                responsive: true
            });

            {block:IfEnableAnimationEffects}
            new WOW().init();
            {/block:IfEnableAnimationEffects}
        });
    </script>
</head>


